<h2 style="color:crimson;">Productos - ejs</h2>

<% if(productos.length > 0){ %>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Titulo</th>
                <th scope="col">Precio</th>
                <th scope="col">url</th>
            </tr>
        </thead>
        <tbody>
            <% for (const producto of productos) {%>
            <tr>
                <td><%=producto.Titulo%></td>
                <td><%=producto.Precio%></td>
                <td><%=producto.url%></td>
            </tr>
            <%};%>
        </tbody>
    </table>
<% } else{ %>
    <h3 class="alert alert-danger">No se encontraron datos</h1>
<% } %>
<div>
    <a href="/" class="btn btn-primary btn-lg active float-right" role="button" aria-pressed="true">Agregar producto</a>
</div>
